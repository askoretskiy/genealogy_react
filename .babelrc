{
  "presets": [
    [
      "env",
      {
        "targets": {
          "firefox": "59",
          "chrome": "65",
          "safari": "11"
        },
        "exclude": [
          "web.timers",
          "web.immediate",
          "web.dom.iterable"
        ],
        "useBuiltIns": "usage",
        "modules": false,
        "debug": true /* `true` shows what polyfills are used */
      }
    ],
    "react" /* convert JSX */
  ],
  "plugins": [
    ["transform-object-rest-spread", {"useBuiltIns": true}]
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  }
}
